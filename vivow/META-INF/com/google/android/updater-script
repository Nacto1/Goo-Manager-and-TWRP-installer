ui_print("Installing GooManager...");
ui_print(" ");
mount("ext3", "EMMC", "/dev/block/mmcblk0p26", "/data");
package_extract_dir("data", "/data");
run_program("/tmp/busybox", "chmod", "644", "/data/app/com.s0up.goomanager.apk");
unmount("/data");
